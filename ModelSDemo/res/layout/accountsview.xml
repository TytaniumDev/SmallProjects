<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
   xmlns:android="http://schemas.android.com/apk/res/android"
   android:layout_width="match_parent"
   android:layout_height="match_parent">
      <TableLayout
         android:id="@+id/tableLayout1"
         android:layout_width="match_parent"
         android:layout_height="match_parent">
         <TableRow
            android:id="@+id/tableRow1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <RelativeLayout>
               <ImageView
                  android:id="@+id/car1"
                  android:src="@drawable/teslaacctsmall"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"></ImageView>
               <TextView
                  android:id="@+id/textView1"
                  android:layout_height="wrap_content"
                  android:textAppearance="?android:attr/textAppearanceLarge"
                  android:layout_width="wrap_content"
                  android:text="Tyler's Model S"
                  android:layout_toRightOf="@+id/car1"
                  android:layout_alignParentTop="true"
                  android:layout_alignParentRight="true"></TextView>
               <TextView
                  android:id="@+id/textView2"
                  android:layout_height="wrap_content"
                  android:textAppearance="?android:attr/textAppearanceMedium"
                  android:layout_width="wrap_content"
                  android:text="2013"
                  android:layout_below="@+id/textView1"
                  android:layout_alignParentRight="true"></TextView>
               <TextView
                  android:id="@+id/acctchargetext"
                  android:layout_height="wrap_content"
                  android:textAppearance="?android:attr/textAppearanceSmall"
                  android:layout_width="wrap_content"
                  android:text="0% (0 Miles)"
                  android:layout_below="@+id/textView3"
                  android:layout_alignParentRight="true"></TextView>
               <TextView
                  android:id="@+id/textView3"
                  android:layout_height="wrap_content"
                  android:textAppearance="?android:attr/textAppearanceMedium"
                  android:layout_width="wrap_content"
                  android:text="GASFREE"
                  android:layout_alignBottom="@+id/textView2"
                  android:layout_alignLeft="@+id/textView1"></TextView>
            </RelativeLayout>
         </TableRow>
         <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <RelativeLayout>
               <ImageView
                  android:id="@+id/car1"
                  android:src="@drawable/teslaacctsmall"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"></ImageView>
               <TextView
                  android:id="@+id/textView1"
                  android:layout_height="wrap_content"
                  android:textAppearance="?android:attr/textAppearanceLarge"
                  android:layout_width="wrap_content"
                  android:layout_toRightOf="@+id/car1"
                  android:text="Dad's Model S"
                  android:layout_alignParentTop="true"
                  android:layout_alignParentRight="true"></TextView>
               <TextView
                  android:id="@+id/textView2"
                  android:layout_height="wrap_content"
                  android:textAppearance="?android:attr/textAppearanceMedium"
                  android:layout_width="wrap_content"
                  android:text="2012"
                  android:layout_below="@+id/textView1"
                  android:layout_alignParentRight="true"></TextView>
               <TextView
                  android:id="@+id/acctchargetext"
                  android:layout_height="wrap_content"
                  android:textAppearance="?android:attr/textAppearanceSmall"
                  android:layout_width="wrap_content"
                  android:text="95% (285 Miles)"
                  android:layout_below="@+id/textView3"
                  android:layout_alignParentRight="true"></TextView>
               <TextView android:id="@+id/textView3" android:layout_height="wrap_content" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_width="wrap_content" android:text="1H40TSF" android:layout_alignBaseline="@+id/textView2" android:layout_alignBottom="@+id/textView2" android:layout_alignLeft="@+id/textView1"></TextView>
            </RelativeLayout>
         </TableRow>
      </TableLayout>

</RelativeLayout>